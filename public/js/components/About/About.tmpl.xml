<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
    <div class="about" style="margin-top: 2rem;">
        <fest:each iterate="data.contributors" index="i">
            <div class="contributor_card">
                <img src="{data.contributors[i].avatarAddress}"></img>
                <span><a href="{data.contributors[i].gitHubUrl}" onclick="event.stopPropagation();"><fest:value>data.contributors[i].name</fest:value></a></span>-<fest:value>data.contributors[i].faculty</fest:value>-<fest:value>data.contributors[i].position</fest:value><br/>
            </div>
        </fest:each>
        <div class="contributor_card" style="border: none;"><a href="{data.fronendtUrl}" onclick="event.stopPropagation();">Репозиторий фронтенда</a></div>
        <div class="contributor_card" style="border: none;"><a href="{data.backendUrl}" onclick="event.stopPropagation();">Репозиторий бекенда</a></div>
        <a href="/menu" data-href="menu" class="about__back">Back</a>
    </div>
</fest:template>