<fest:template xmlns:fest="http://fest.mail.ru" context_name="data">
        <div id="chat" class="chat">
            <div id="prev_tab" class="prev_tab">PREV</div>
            <div id="tab" class="tab">Current tab</div>
            <div id="next_tab" class="next_tab">NEXT</div>
            <div id="new_tab" class="new_tab">+</div>
            <div id="new_login" class="new_login_modal" hidden="true">
                <div class="new_login_modal_content">
                    <input id="new_login_input" type="text" name="login" placeholder="Input receiver"></input>
                    <div id="new_login_btn"><button>Find</button></div>
                </div>
            </div>
            <div class="roll_btn"> -- </div>
            <div class="close_btn"> X   </div>
            <div id="chat_body" class="chat_body">
                <div class="load_more">BTN</div>
                <div class="item item_to_me">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
                <div class="item">jopa</div>
            </div>
            <div id="text_input" class="text_input"><input type="text" name="message" placeholder="Write message"></input></div>
            <div id="send_btn" class="send_btn"><button>Send</button></div>
        </div>
</fest:template>