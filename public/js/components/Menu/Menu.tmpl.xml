<fest:template xmlns:fest="http://fest.mail.ru" context_name="menu_titles">
<div class="main-menu">
    <div class="menu-items">
        <fest:each iterate="menu_titles" index="i">
            <div class="menu-button">
                <a data-href="{menu_titles[i].url}" href="/{menu_titles[i].url}">
                    <fest:value>menu_titles[i].title</fest:value>
                </a>
            </div>
        </fest:each>
    </div>
</div>
</fest:template>